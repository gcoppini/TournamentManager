@model ResultViewModel

@{
    ViewData["Title"] = "Resultado";
}

<div class="row">
   <div class="col-md-12">
        <h2>Resultado</h2>
        <table class="table">
        <thead>
            <tr>
                <th>
                     @Html.DisplayNameFor(model => model.Competitors[0].Ranking)
                </th>
                <th>
                     @Html.DisplayNameFor(model => model.Competitors[0].Name)
                </th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model.Competitors) 
            {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.Ranking)
                </td>
                <td>
                <td>
                    @Html.DisplayFor(modelItem => item.Name)
                </td>
                </td>
            </tr>
            }
        </tbody>
        </table>
    </div>
</div>